{
	"ignore": [
		".git",
		".eslintrc",
		"node_modules/**",
		"src/client/**",
		"test/**",
		"public/**",
		"*.test.js",
		"webpack.*.js"
	],
	"watch": ["src/server/", ".env", "src/utils/@types"],
	"ext": "ts, js",
	"exec": "cross-env NODE_ENV=development node --inspect -r ts-node/register -r tsconfig-paths/register ./src/server/index.ts"
}
